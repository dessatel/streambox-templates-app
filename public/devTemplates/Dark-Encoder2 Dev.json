                {
                  "type": "input",
                  "label": "Destination Port",
                  "mapping": "port",
                  "endLabel": "UDP (default: 1770)"
                },
                {
                  "type": "input",
                  "label": "IP Packet Size",
                  "mapping": "packetsize",
                  "endLabel": "bytes (default: 1392)"
                },
                {
                  "type": "button",
                  "action": "submitLocalForm",
                  "size": "small",
                  "label": "Apply Changes"
                }
              ]
            },
            {
              "title": "Forward Error Correction",
              "apiSrc": "/REST/encoder/@JS_PORT@/network.json",
              "postEndpoint": "/REST/encoder/@JS_PORT@/network.json",
