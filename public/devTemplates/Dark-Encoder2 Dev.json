                    },
                    {
                      "type": "mappedText",
                      "label": "SDI errors",
                      "mapping": "sdi_error"
                    },
                    {
                      "type": "mappedText",
                      "label": "Frame Delay",
                      "mapping": "frame_delay"
                    }
                  ]
                }
              ]
            },
            {
              "title": "Network",
              "apiSrc": "/REST/encoder/@JS_PORT@/status.json",
              "fields": [
                {
                  "type": "mappedText",
                  "label": "Encoder IP",
                  "mapping": "c_encoderIP"
                },
                {
                  "type": "mappedText",
                  "label": "Decoder IP",
                  "mapping": "c_decoderIP"
                },
                {
                  "type": "mappedText",
                  "label": "Packet Size",
                  "mapping": "c_packetsize"
                },
                {
                  "type": "mappedText",
                  "label": "Shuffle",
                  "mapping": "cshuffle"
                },
                {
                  "type": "mappedText",
                  "label": "Buffer",
                  "mapping": "cbuffer"
                },
                {
                  "type": "mappedText",
                  "label": "Bytes",
