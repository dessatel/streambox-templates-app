{
  "template": {
    "darkMode": true,
    "backgroundRefreshTime": 7000,
    "navbar": {
      "routes": [
        {
          "routeName": "Multichannel Selection",
          "isMultichannelPage": true,
          "containersStyle": {
            "numberOfColumns": 2
          },
          "containers": [
            {
              "title": "DAB_CX-1",
              "apiSrc": "/REST/encoder/status.json",
              "postEndpoint": "/REST/encoder/metadata.json",
              "backgroundColor": "gainsboro",
              "type": "multichannel-container",
              "fields": [
                {
                  "type": "plainText",
                  "text": "Bitrate: 3020 Kbps"
                },
                {
                  "type": "plainText",
                  "text": "Signal: 1080p23.98"
                },
                {
                  "type": "plainText",
                  "text": "Title: CNN / Streambox 8Mbps Test"
                },
                {
                  "type": "plainText",
                  "text": "Dest: 127.0.0.1"
                },
                {
                  "type": "plainText",
                  "text": "Port: 1770"
                },
                {
                  "type": "button",
                  "presetSrc": "/REST/encoder/presets.json",
                  "action": "applyPreset",
                  "size": "big",
                  "backgroundColor": "#333",
                  "label": "Turn Off"
                }
              ]
            },
            {
              "title": "DAB_CX-2",
              "apiSrc": "/REST/encoder/status.json",
              "postEndpoint": "/REST/encoder/metadata.json",
              "backgroundColor": "gainsboro",
              "type": "multichannel-container",
              "fields": [
                {
                  "type": "plainText",
                  "text": "Bitrate: 3020 Kbps"
                },
                {
                  "type": "plainText",
                  "text": "Signal: 1080p23.98"
                },
                {
                  "type": "plainText",
                  "text": "Title: CNN / Streambox 8Mbps Test"
                },
                {
                  "type": "plainText",
                  "text": "Dest: 127.0.0.1"
                },
                {
                  "type": "plainText",
                  "text": "Port: 1770"
                },
                {
                  "type": "button",
                  "presetSrc": "/REST/encoder/presets.json",
                  "action": "applyPreset",
                  "size": "big",
                  "backgroundColor": "#333",
                  "label": "Turn Off"
                }
              ]
            }
          ]
        },
        {
          "routeName": "Session @JS_ENC@",
          "containersStyle": {
            "numberOfColumns": 2
          },
          "containers": [
            {
              "title": "Video",
              "apiSrc": [
                "/REST/encoder/@JS_PORT@/status.json",
                "/REST/encoder/@JS_PORT@/audio.json"
              ],
              "containerStyle": {
                "stretchVertically": 2
              },
              "fields": [
                {
                  "type": "video",
                  "hasVUMeter": true,
                  "audioLevelEndpoint": "/REST/encoder/@JS_PORT@/audiolevels.json",
                  "previewImageRoute": "/REST/encoder/@JS_PORT@/preview/JPG.jpeg"
                },
                {
                  "type": "mappedText",
                  "label": "Source Video Format",
                  "mapping": "inputsignal",
                  "inlineElements": [
                    {
                      "type": "mappedText",
                      "label": "Network Mode",
                      "mapping": "cnetworkmode"
                    },
                    {
                      "type": "mappedText",
                      "label": "Bitrate",
                      "mapping": "cur_bitrate"
                    },
                    {
                      "type": "mappedText",
                      "label": "Quality",
                      "mapping": "vquality"
                    }
                  ]
                },
                {
                  "type": "mappedText",
                  "label": "Dropped Captured Frames",
                  "mapping": "dropCapture",
                  "inlineElements": [
                    {
                      "type": "mappedText",
                      "label": "Dropped Encoder Frames",
                      "mapping": "dropEnc"
                    },
                    {
                      "type": "mappedText",
                      "label": "SDI errors",
                      "mapping": "sdi_error"
                    },
                    {
                      "type": "mappedText",
                      "label": "Frame Delay",
                      "mapping": "frame_delay"
                    }
                  ]
                }
              ]
            },
            {
              "title": "Start Streaming",
              "apiSrc": [
                "/REST/encoder/@JS_PORT@/status.json",
                "/REST/encoder/@JS_PORT@/presets.json"
              ],
              "fields": [
                {
                  "type": "button",
                  "size": "giant",
                  "mapping": "isStreaming",
                  "action": "toggleStreaming"
                },
                {
                  "type": "presetSelect",
                  "label": "Preset",
                  "btnPresetSrc": "/REST/encoder/@JS_PORT@/presets.json",
                  "btnLabel": "Apply Preset",
                  "btnAction": "applyPreset",
                  "mapping": "none"
                }
              ]
            },
            {
              "type": "sessionsPanel"
            }
          ]
        },
        {
          "routeName": "Info",
          "containersStyle": {
            "numberOfColumns": 2
          },
          "containers": [
            {
              "title": "Video",
