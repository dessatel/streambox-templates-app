                  "btnAction": "applyPreset",
                  "mapping": "none"
                }
              ]
            }
          ]
        },
        {
          "routeName": "Network",
          "containersStyle": {
            "numberOfColumns": 2
          },
          "containers": [
            {
              "title": "Bitrate and Buffer Settings",
              "apiSrc": [
                "/REST/encoder/@JS_PORT@/network.json",
                "/REST/encoder/@JS_PORT@/video.json"
              ],
              "type": "form",
              "postEndpoint": "/REST/encoder/@JS_PORT@/video.json",
              "fields": [
                {
                  "type": "input",
                  "label": "Target Bitrate",
                  "mapping": "TargetBitrate",
                  "endLabel": "kbps"
                },
                {
                  "type": "input",
                  "label": "VBR Buffer",
                  "mapping": "VBR",
                  "endLabel": "seconds"
                },
                {
                  "type": "checkbox",
                  "label": "Packet Stuffing",
                  "mapping": "stuffing"
                },
                {
                  "type": "button",
                  "action": "submitLocalForm",
                  "size": "small",
                  "label": "Apply Changes"
                }
              ]
            },
            {
              "title": "Transport Settings",
              "apiSrc": "/REST/encoder/@JS_PORT@/transport.json",
              "postEndpoint": "/REST/encoder/@JS_PORT@/transport.json",
              "type": "form",
              "containerStyle": {
                "stretchVertically": 2
              },
              "fields": [
                {
                  "type": "select",
                  "label": "Protocol",
                  "mapping": "ldmp-use"
                },
                {
                  "type": "input",
                  "label": "CWND",
                  "mapping": "ldmp-cwnd"
                },
                {
                  "type": "input",
                  "label": "CWND MIN",
                  "mapping": "ldmp-cwndmin"
                },
                {
                  "type": "input",
                  "label": "CWND MAX",
                  "mapping": "ldmp-cwndmax"
                },
                {
                  "type": "input",
                  "label": "ACK Timeout",
                  "mapping": "ldmp-ackto"
                },
                {
                  "type": "input",
                  "label": "Send Timeout",
                  "mapping": "ldmp-sndto"
                },
                {
                  "type": "input",
                  "label": "RTT Multiplier",
                  "mapping": "ldmp-rttm"
                },
                {
                  "type": "input",
                  "label": "Pre-buffer for packet delay (jitter)",
                  "mapping": "ldmp-jitter2p"
                },
                {
                  "type": "input",
                  "label": "Shaping Percent",
                  "mapping": "ldmp-maxbrt"
                },
                {
                  "type": "button",
                  "action": "submitLocalForm",
                  "size": "small",
                  "label": "Apply Changes"
                }
              ]
            },
            {
              "title": "Target Decoder",
              "apiSrc": [
                "/REST/encoder/@JS_PORT@/network.json",
                "/REST/encoder/@JS_PORT@/status.json"
