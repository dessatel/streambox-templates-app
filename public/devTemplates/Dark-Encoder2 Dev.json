                },
                {
                  "type": "mappedText",
                  "label": "Packets Sent",
                  "mapping": "sent_packets"
                },
                {
                  "type": "mappedText",
                  "label": "Transmission",
                  "mapping": "is_ldmp"
                }
              ]
            },
            {
