                },
                {
                  "type": "input",
                  "label": "Producer",
                  "mapping": "Meta_Producer"
                },
                {
                  "type": "input",
                  "label": "DRM",
                  "mapping": "Meta_Network1"
                },
                {
                  "type": "button",
                  "action": "submitLocalForm",
                  "size": "small",
                  "label": "Apply Changes"
                }
              ]
            }
          ]
        },
