                },
                {
                  "type": "select",
                  "label": "Shuffle",
                  "mapping": "shuffle"
                },
                {
                  "type": "button",
                  "action": "submitLocalForm",
                  "size": "small",
                  "label": "Apply Changes"
                }
              ]
            }
          ]
