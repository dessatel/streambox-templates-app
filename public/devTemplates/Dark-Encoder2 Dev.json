                  "endLabel": "(default: 2-ch)"
                },
                {
                  "type": "select",
                  "label": "Sample Rate",
                  "mapping": "SampleRate",
                  "endLabel": "(default: 48000 Hz)"
                },
                {
                  "type": "button",
                  "action": "submitLocalForm",
                  "size": "small",
                  "label": "Apply Changes"
                }
              ]
            }
