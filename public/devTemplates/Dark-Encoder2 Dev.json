        {
          "routeName": "Config",
          "containersStyle": {
            "numberOfColumns": 2
          },
          "containers": [
            {
              "title": "Video Settings",
              "apiSrc": "/REST/encoder/video.json",
              "postEndpoint": "/REST/encoder/@JS_PORT@/video.json",
              "type": "form",
              "fields": [
                {
                  "type": "select",
                  "label": "Video Resolution",
                  "mapping": "Resolution"
                },
                {
                  "type": "select",
                  "label": "Color Profile",
                  "mapping": "ColorSpace",
                  "endLabel": "(default: 4:2:2)"
                },
                {
                  "type": "select",
                  "label": "Framerate",
                  "mapping": "FrameRate",
                  "endLabel": "(default: Full)"
                },
                {
                  "type": "input",
                  "label": "Key Frames",
                  "mapping": "KeyFrame",
                  "endLabel": "(default: 300 frames)"
                },
                {
                  "type": "checkbox",
                  "label": "Advanced Profile",
                  "mapping": "RecycleIntra",
                  "endLabel": "(Default: Off)"
                },
                {
                  "type": "select",
                  "label": "Bit Depth",
                  "mapping": "BitDepth",
                  "endLabel": "(Default: 10-bit)"
                },
                {
                  "type": "select",
                  "label": "Color Space",
                  "mapping": "NativeColorSpace",
                  "endLabel": "For RGB 4:4:4 only"
                },
                {
                  "type": "input",
                  "label": "Dynamic Resolution Quality",
                  "mapping": "DynamicRes",
                  "endLabel": "set to 0 to turn off"
                },
                {
                  "type": "button",
                  "action": "submitLocalForm",
                  "size": "small",
                  "label": "Apply Changes"
                }
              ]
            },
